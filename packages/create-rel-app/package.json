{
  "name": "create-rel-app",
  "version": "0.9.2",
  "description": "Create rel powered API with one command",
  "author": "Ian Hunter <ian@rel.run>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/create-rel-app.js",
  "types": "dist/create-rel-app.d.ts",
  "files": [
    "template/**/*",
    "dist/**/*"
  ],
  "bin": {
    "create-rel-app": "./dist/create-rel-app.js"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc && yarn dist",
    "build:watch": "tsc --watch",
    "dist": "yarn dist:chmod && yarn dist:copy",
    "dist:copy": "cp -r template dist",
    "dist:chmod": "chmod +x ./dist/create-rel-app.js"
  },
  "gitHead": "5e37d0fda7603768722e40c2e0f4c409533ff26a",
  "dependencies": {
    "@reldb/run": "^0.12.2",
    "chalk": "^4.1.0",
    "fs-extra": "^9.1.0",
    "fs-utils": "^0.7.0",
    "inquirer": "^8.0.0",
    "ora": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1"
  }
}
