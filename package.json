{
  "name": "rel",
  "version": "0.1.1",
  "description": "React animated SMS conversation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "ttsc",
    "test": "jest --watch --detectOpenHandles --runInBand",
    "test:ci": "jest --detectOpenHandles --runInBand",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "dev": "nodemon --watch '**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ttsc --watch'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/reltool/rel.git"
  },
  "keywords": [
    "react",
    "conversation",
    "sms"
  ],
  "author": "Ian Hunter",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reltool/rel/issues"
  },
  "homepage": "https://github.com/reltool/rel#readme",
  "dependencies": {
    "@google/maps": "^1.1.3",
    "@graphql-tools/merge": "^6.2.7",
    "@graphql-tools/schema": "^7.1.3",
    "bcryptjs": "^2.4.3",
    "fastify": "^3.11.0",
    "format-graphql": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mercurius": "7",
    "moment": "^2.29.1",
    "neo4j-driver": "^4.2.2",
    "pluralize": "^8.0.0",
    "slugify": "^1.4.6",
    "titleize": "^2.1.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.0",
    "boxen": "^5.0.0",
    "env-cmd": "^10.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.5",
    "typescript-transform-paths": "^2.2.3"
  }
}
